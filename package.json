{
  "name": "curso-react-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "gh-pages-deploy": "^0.4.2",
    "react-scripts": "0.8.3"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "gh-pages-deploy": {
    "staticpath": "build",
    "prep": [
      "build"
    ],
    "post": [
      "clear"
    ],
    "noprompt": false
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "clear": "rimraf build",
    "deploy": "gh-pages-deploy"
  }
}
